rm -f conftest.val
else
  ac_cv_sizeof_unsigned_long=0
fi
fi
echo "$as_me:12101: result: $ac_cv_sizeof_unsigned_long" >&5
echo "${ECHO_T}$ac_cv_sizeof_unsigned_long" >&6
