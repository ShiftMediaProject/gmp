  (eval $ac_link) 2>&5
  ac_status=$?
  echo "$as_me:7667: \$? = $ac_status" >&5
  (exit $ac_status); } &&
         { ac_try='test -s conftest$ac_exeext'
  { (eval echo "$as_me:7670: \"$ac_try\"") >&5
  (eval $ac_try) 2>&5
  ac_status=$?
  echo "$as_me:7673: \$? = $ac_status" >&5
  (exit $ac_status); }; }; then

